issn = {0002-9262},
journal = {American Journal of Epidemiology},
keywords = {epidemiologic methods,maximum likelihood,modeling,penalized estimation,regression,statistics},
month = {jan},
number = {2},
pages = {252--260},
title = {{Maximum Likelihood, Profile Likelihood, and Penalized Likelihood: A Primer}},
url = {https://academic.oup.com/aje/article-lookup/doi/10.1093/aje/kwt245},
volume = {179},
year = {2014}
}
