month = {apr},
number = {2},
pages = {87--94},
title = {{Recruitment of common sardine (Strangomera bentincki) and anchovy (Engraulis ringens) off central-south Chile in the 1990s and the impact of the 1997–1998 El Ni{\~{n}}o}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0990744002011580},
