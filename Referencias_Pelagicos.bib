journal = {Fisheries Research},
keywords = {Anchovy,Reproductive potential,Sardine,Spawning,Spawning periods},
month = {dec},
pages = {96--102},
publisher = {Elsevier},
title = {{Variation in the spawning periods of Engraulis ringens and Strangomera bentincki off the coasts of Chile: A quantitative analysis}},
