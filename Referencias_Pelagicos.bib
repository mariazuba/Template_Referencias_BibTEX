keywords = {Central-south Chile,DEPM,Daily egg production,Reproductive parameters,Small pelagic fish,Spawning},
month = {sep},
number = {2-3},
pages = {228--240},
title = {{Spawning, daily egg production, and spawning stock biomass estimation for common sardine (Strangomera bentincki) and anchovy (Engraulis ringens) off central southern Chile in 2002}},
